
@{
    ViewBag.Title = "QuanLyQuanAn";
    Layout = "~/Views/Shared/_LayoutPageAmin.cshtml";
    List<ShipFood.Models.tbQuanAn> datas = ViewBag.datas;
}

<div class="content-body">
    <div class="container-fluid">

        <div class="app-card-body">
            <div class="table-responsive">
                <table class="table table-striped table-bordered first">
                    <thead>
                        <tr>
                            <th class="cell" style="text-align: center;">ID</th>
                            <th class="cell" style="text-align: center;">Họ tên</th>
                            <th class="cell" style="text-align: center;">Username</th>
                            <th class="cell" style="text-align: center;">Ví tiền</th>
                        </tr>
                    </thead>
                    <tbody>
                        @foreach (var i in datas)
                        {
                            <tr>
                                <td style="text-align: center;" class="cell">#@i.userid</td>
                                <td class="cell"><span class="truncate">@i.tenquanan</span></td>
                                <td class="cell">@i.tbUser.username</td>
                                <td class="cell">@Math.Round((Double.Parse((i.tbUser.vitien / 1000).ToString())))K VNĐ</td>
                                
                            </tr>
                        }


                    </tbody>
                </table>
            </div><!--//table-responsive-->

        </div><!--//app-card-body-->


    </div>
</div>